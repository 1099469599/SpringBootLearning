<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cml.springboot.sample.db.BaseAccessTokenMapper">
  <resultMap id="BaseResultMap" type="com.cml.springboot.sample.bean.BaseAccessToken">
    <result column="access_token" jdbcType="OTHER" property="accessToken" />
    <result column="expire_time" jdbcType="OTHER" property="expireTime" />
    <result column="create_date" jdbcType="OTHER" property="createDate" />
    <result column="update_time" jdbcType="OTHER" property="updateTime" />
  </resultMap>
  <insert id="insert" parameterType="com.cml.springboot.sample.bean.BaseAccessToken">
    insert into access_token (access_token, expire_time, create_date, 
      update_time)
    values (#{accessToken,jdbcType=OTHER}, #{expireTime,jdbcType=OTHER}, #{createDate,jdbcType=OTHER}, 
      #{updateTime,jdbcType=OTHER})
  </insert>
  <insert id="insertSelective" parameterType="com.cml.springboot.sample.bean.BaseAccessToken">
    insert into access_token
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="accessToken != null">
        access_token,
      </if>
      <if test="expireTime != null">
        expire_time,
      </if>
      <if test="createDate != null">
        create_date,
      </if>
      <if test="updateTime != null">
        update_time,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="accessToken != null">
        #{accessToken,jdbcType=OTHER},
      </if>
      <if test="expireTime != null">
        #{expireTime,jdbcType=OTHER},
      </if>
      <if test="createDate != null">
        #{createDate,jdbcType=OTHER},
      </if>
      <if test="updateTime != null">
        #{updateTime,jdbcType=OTHER},
      </if>
    </trim>
  </insert>
</mapper>