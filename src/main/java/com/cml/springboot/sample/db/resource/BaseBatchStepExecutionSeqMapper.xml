<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cml.springboot.sample.db.BaseBatchStepExecutionSeqMapper">
  <resultMap id="BaseResultMap" type="com.cml.springboot.sample.bean.BaseBatchStepExecutionSeq">
    <result column="ID" jdbcType="OTHER" property="id" />
    <result column="UNIQUE_KEY" jdbcType="OTHER" property="uniqueKey" />
  </resultMap>
  <insert id="insert" parameterType="com.cml.springboot.sample.bean.BaseBatchStepExecutionSeq">
    <selectKey keyProperty="id" order="BEFORE" resultType="java.math.BigInteger">
      SELECT LAST_INSERT_ID()
    </selectKey>
    insert into BATCH_STEP_EXECUTION_SEQ (ID, UNIQUE_KEY)
    values (#{id,jdbcType=OTHER}, #{uniqueKey,jdbcType=OTHER})
  </insert>
  <insert id="insertSelective" parameterType="com.cml.springboot.sample.bean.BaseBatchStepExecutionSeq">
    <selectKey keyProperty="id" order="BEFORE" resultType="java.math.BigInteger">
      SELECT LAST_INSERT_ID()
    </selectKey>
    insert into BATCH_STEP_EXECUTION_SEQ
    <trim prefix="(" suffix=")" suffixOverrides=",">
      ID,
      <if test="uniqueKey != null">
        UNIQUE_KEY,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      #{id,jdbcType=OTHER},
      <if test="uniqueKey != null">
        #{uniqueKey,jdbcType=OTHER},
      </if>
    </trim>
  </insert>
</mapper>